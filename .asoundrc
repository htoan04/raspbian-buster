pcm.mid {
        type plug
        slave {
            pcm {
                type bluealsa
                device C8:84:47:43:64:4B
                profile "a2dp"
            }
        }
        hint {
            show on
            description "YAS-107 Yamaha"
        }
     }

pcm.array
{
    type hw
    card CameraB409241
    format S16_LE
    rate 16000
}

pcm.usb
{
    type hw
    card 2
}

pcm.internal
{
    type hw
    card 0
}

pcm.array_gain {
  type softvol
  slave {
    pcm "array"
  }
  control {
    name "Mic Gain"
    count 2
  }
  min_dB -10.0
  max_dB 5.0
}

pcm.cap {
  type plug
  slave {
    pcm "array_gain"
    channels 4
  }
  route_policy sum
}


pcm.!default
{
    type asym
    playback.pcm
    {
        type plug
        slave.pcm "internal"
    }
    capture.pcm
    {
        type plug
        slave.pcm "cap"
    }
}

ctl.!default {
	type bluealsa
}
    capture.pcm 
    {
        type plug
        slave.pcm "cap"
    }
}
